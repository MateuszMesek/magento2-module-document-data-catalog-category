            <argument name="attributeReturnTypeResolver" xsi:type="object">MateuszMesek\DocumentDataCatalogCategory\AttributeReturnTypeResolver</argument>
        </arguments>
    </virtualType>
    <virtualType name="MateuszMesek\DocumentDataCatalogCategory\AttributeValueResolver" type="MateuszMesek\DocumentDataEav\AttributeValueResolver\ByAttributeCode">
        <arguments>
            <argument name="defaultResolver" xsi:type="object">MateuszMesek\DocumentDataCatalogCategory\AttributeValueResolver\DefaultResolver</argument>
