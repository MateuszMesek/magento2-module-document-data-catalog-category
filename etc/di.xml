    <virtualType name="MateuszMesek\DocumentDataCatalogCategory\AttributeReturnTypeResolver" type="MateuszMesek\DocumentDataEav\AttributeReturnTypeResolver\ByAttributeCode"/>
    <type name="MateuszMesek\DocumentDataCatalogCategory\Command\GetDocumentData">
