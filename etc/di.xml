    <virtualType name="MateuszMesek\DocumentDataCatalogCategory\Command\GetDocumentNodes\AttributeResolver" type="MateuszMesek\DocumentDataEav\DocumentNodesResolver">
        <arguments>
            <argument name="entityType" xsi:type="const">Magento\Catalog\Model\Category::ENTITY</argument>
